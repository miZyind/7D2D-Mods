<Project>
  <ItemDefinitionGroup>
    <Reference Private="false" />
    <ProjectReference Private="false" />
  </ItemDefinitionGroup>
  <PropertyGroup>
    <!-- Configuration -->
    <Configurations>Build</Configurations>
    <Platform>AnyCPU</Platform>
    <DebugSymbols>false</DebugSymbols>
    <DebugType>none</DebugType>
    <Optimize>true</Optimize>
    <ErrorReport>none</ErrorReport>
    <WarningLevel>0</WarningLevel>
    <WarningsAsErrors></WarningsAsErrors>
    <TreatWarningsAsErrors>false</TreatWarningsAsErrors>
    <AutoGenerateBindingRedirects>false</AutoGenerateBindingRedirects>
    <GenerateSerializationAssemblies>Off</GenerateSerializationAssemblies>
    <!-- Information -->
    <Company>miZyind.dev</Company>
    <Copyright>Copyright Â© miZyind</Copyright>
    <!-- Output -->
    <OutDir>bin</OutDir>
    <OutputType>Library</OutputType>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <!-- Folder -->
    <SteamFolder>C:\Program Files (x86)\Steam</SteamFolder>
    <GameFolder>$(SteamFolder)\steamapps\common\7 Days To Die</GameFolder>
    <ManagedFolder>$(GameFolder)\7DaysToDie_Data\Managed</ManagedFolder>
    <ModFolder>$(GameFolder)\Mods</ModFolder>
  </PropertyGroup>
  <ItemGroup>
    <Reference Include="UnityEngine" HintPath="$(ManagedFolder)\UnityEngine.dll" />
    <Reference Include="UnityEngine.CoreModule" HintPath="$(ManagedFolder)\UnityEngine.CoreModule.dll" />
    <Reference Include="Assembly-CSharp" HintPath="$(ManagedFolder)\Assembly-CSharp.dll" />
    <Reference Include="0Harmony" HintPath="$(ManagedFolder)\0Harmony.dll" />
  </ItemGroup>
</Project>
